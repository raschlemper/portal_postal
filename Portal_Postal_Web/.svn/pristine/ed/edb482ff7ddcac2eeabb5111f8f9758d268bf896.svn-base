package Coleta.Entidade;

/**
 *
 * @author SCC4
 */
public class ColetaFixa {

    private int idColetaFixa;
    private int idCliente;
    private int idColetador;
    private int idTipo;
    private int fixo;
    private String hora;

    public ColetaFixa(int idColetaFixa, int idCliente, int idColetador, int idTipo, int fixo, String hora) {
        this.idColetaFixa = idColetaFixa;
        this.idCliente = idCliente;
        this.idColetador = idColetador;
        this.idTipo = idTipo;
        this.fixo = fixo;
        this.hora = hora;
    }

    public int getFixo() {
        return fixo;
    }

    public void setFixo(int fixo) {
        this.fixo = fixo;
    }

    public String getHora() {
        return hora;
    }

    public void setHora(String hora) {
        this.hora = hora;
    }

    public int getIdCliente() {
        return idCliente;
    }

    public void setIdCliente(int idCliente) {
        this.idCliente = idCliente;
    }

    public int getIdColetaFixa() {
        return idColetaFixa;
    }

    public void setIdColetaFixa(int idColetaFixa) {
        this.idColetaFixa = idColetaFixa;
    }

    public int getIdColetador() {
        return idColetador;
    }

    public void setIdColetador(int idColetador) {
        this.idColetador = idColetador;
    }

    public int getIdTipo() {
        return idTipo;
    }

    public void setIdTipo(int idTipo) {
        this.idTipo = idTipo;
    }

}
