package Entidade;

/**
 *
 * @author scc4
 */
public class ServicoCombo {
    
    private int codigo_ECT_combo;
    private int codigo_ECT_servico;
    private int ar;
    private int mp;
    private int vd;
    private int rg;
    private String nome_combo;

    public ServicoCombo(int codigo_ECT_combo, int codigo_ECT_servico, int ar, int mp, int vd, int rg, String nome_combo) {
        this.codigo_ECT_combo = codigo_ECT_combo;
        this.codigo_ECT_servico = codigo_ECT_servico;
        this.ar = ar;
        this.mp = mp;
        this.vd = vd;
        this.rg = rg;
        this.nome_combo = nome_combo;
    }

    public int getCodigo_ECT_combo() {
        return codigo_ECT_combo;
    }

    public void setCodigo_ECT_combo(int codigo_ECT_combo) {
        this.codigo_ECT_combo = codigo_ECT_combo;
    }

    public int getCodigo_ECT_servico() {
        return codigo_ECT_servico;
    }

    public void setCodigo_ECT_servico(int codigo_ECT_servico) {
        this.codigo_ECT_servico = codigo_ECT_servico;
    }

    public int getAr() {
        return ar;
    }

    public void setAr(int ar) {
        this.ar = ar;
    }

    public int getMp() {
        return mp;
    }

    public void setMp(int mp) {
        this.mp = mp;
    }

    public int getVd() {
        return vd;
    }

    public void setVd(int vd) {
        this.vd = vd;
    }

    public int getRg() {
        return rg;
    }

    public void setRg(int rg) {
        this.rg = rg;
    }

    public String getNome_combo() {
        return nome_combo;
    }

    public void setNome_combo(String nome_combo) {
        this.nome_combo = nome_combo;
    }
    
}
