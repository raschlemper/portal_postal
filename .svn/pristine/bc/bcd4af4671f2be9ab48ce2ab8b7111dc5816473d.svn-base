<form name="veiculoManutencaoForm" novalidate>
    <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" ng-click="cancel()">&times;</button>
        <h4 class="modal-title">Visualizar Manutenção Veículo</h4>
    </div>
    <div class="modal-body">
        <div class="bootbox-body">
            <div class="row">
                <div class="col-md-4 text-right">
                    <label>Veículo:</label>
                </div>
                <div class="col-md-8">{{veiculoManutencao.veiculo.placa | placa}}</div>
            </div>
            <div class="row">
                <div class="col-md-4 text-right">
                    <label>Tipo:</label>
                </div>
                <div class="col-md-8">{{veiculoManutencao.tipo.descricao}}</div>
            </div>
            <div class="row">
                <div class="col-md-4 text-right">
                    <label>KM:</label>
                </div>
                <div class="col-md-8">{{veiculoManutencao.quilometragem | number}}</div>
            </div>
            <div class="row">
                <div class="col-md-4 text-right">
                    <label>Valor:</label>
                </div>
                <div class="col-md-8">{{veiculoManutencao.valor | currency}}</div>
            </div>
            <div class="row">
                <div class="col-md-4 text-right">
                    <label>Data Agendamento:</label>
                </div>
                <div class="col-md-8">{{veiculoManutencao.dataAgendamento | date : 'dd/MM/yyyy'}}</div>
            </div>
            <div class="row">
                <div class="col-md-4 text-right">
                    <label>Data Manutenção</label>
                </div>
                <div class="col-md-8">{{veiculoManutencao.dataManutencao | date : 'dd/MM/yyyy'}}</div>
            </div>
            <div class="row">
                <div class="col-md-4 text-right">
                    <label>Descrição:</label>
                </div>
                <div class="col-md-8 angular-with-newlines">{{veiculoManutencao.descricao}}</div>
            </div>
        </div>
    </div>
    <div class="modal-footer">
        <button type="button" class="btn btn-warning" ng-click="editar()">
            <i class="fa fa-lg fa-pencil fa-spc"></i> EDITAR
        </button>
        <button type="button" class="btn btn-default" ng-click="cancel()">
            <i class="fa fa-lg fa-times fa-spc"></i> CANCELAR
        </button>
    </div>
</form>    

  