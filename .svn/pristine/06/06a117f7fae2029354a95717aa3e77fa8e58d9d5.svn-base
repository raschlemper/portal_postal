<div ng-controller="ModalLancamentoProgramadoEditarController">
    <div ng-include="'partials/financeiro/lancamentoProgramado/modalLancamentoProgramadoEditar.html'" ng-if="stepTo === 'editar'"></div>
    <div ng-controller="ModalLancamentoProgramadoGerarController" ng-if="stepTo === 'lancar' || stepTo === 'ratear'">
        <div ng-include="'partials/financeiro/lancamentoProgramado/modalLancamentoProgramadoGerar.html'"  ng-if="stepTo === 'lancar'"></div>
        <div ng-include="'partials/financeiro/lancamentoProgramado/modalLancamentoProgramadoRatear.html'" ng-if="stepFrom === 'lancar' && stepTo === 'ratear'" ng-controller="ModalLancamentoProgramadoRatearController"></div>
    </div>
    <div ng-controller="ModalLancamentoProgramadoParcelarController" ng-if="stepTo === 'parcelar' || stepTo === 'lancarParcelar' || stepTo === 'ratear'">
        <div ng-include="'partials/financeiro/lancamentoProgramado/modalLancamentoProgramadoParcelar.html'" ng-if="stepTo === 'parcelar'"></div>
        <div ng-controller="ModalLancamentoProgramadoGerarController" ng-if="stepTo === 'lancarParcelar' || stepTo === 'ratear'">
            <div ng-include="'partials/financeiro/lancamentoProgramado/modalLancamentoProgramadoGerar.html'" ng-if="stepTo === 'lancarParcelar'"></div>
            <div ng-include="'partials/financeiro/lancamentoProgramado/modalLancamentoProgramadoRatear.html'" ng-if="stepFrom === 'lancarParcelar' && stepTo === 'ratear'" ng-controller="ModalLancamentoProgramadoRatearController"></div>
        </div>
    </div>
</div>

