    public String getAdicionais() {
        StringBuilder sb = new StringBuilder("");
        if (ar == 1) {
            sb.append("AR");
        }
        if (mp == 1) {
            sb.append("MP");
        }
        if (vd > 0) {
            sb.append("VD");
        }
        if (rm == 1) {
            sb.append("RM");
        }
        if (pr == 1) {
            sb.append("PR");
        }
        return sb.toString();
    }

