    public String getDataMatrixEct() {
        StringBuilder sb = new StringBuilder();
        sb.append("http://datamatrix.kaywa.com/img.php?s=5&d=");
        sb.append(Util.FormataString.formatarCep("########", destinatario_cep));
        sb.append("00000");
        sb.append(Util.FormataString.formatarCep("########", remetente_cep));
        sb.append("00000");
        sb.append(Util.FormataString.calculaValidadorCep(destinatario_cep));
        sb.append("51");
        sb.append(sro);
        sb.append(Util.FormataString.preencheStringCom(getAdicionais(), "0", 8, 1));
        sb.append(Util.FormataString.preencheStringCom(cartao_postagem, "0", 10, -1));
        sb.append(codido_ect);
        sb.append("00");
        sb.append(Util.FormataString.preencheStringCom(destinatario_numero, " ", 5, 1));
        sb.append(Util.FormataString.preencheStringCom(destinatario_complemento, " ", 20, 1));
        sb.append((vd + "").replace(".", ""));
        sb.append("000000000000");
        sb.append("-00.000000");
        sb.append("-00.000000");
        sb.append("|");
        sb.append(Util.FormataString.preencheStringCom("Reserva para cliente ", " ", 30, 1));

        return sb.toString().replace(" ", "%20");
    }

