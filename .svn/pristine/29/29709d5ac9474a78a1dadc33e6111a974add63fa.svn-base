package Entidade;

/**
 *
 * @author RICARDINHO
 */
public class ServicoAbrangencia {
    
    private String servico;
    private int cep_inicial;
    private int cep_final;
    private int faixa_suspensa;
    private int servico_suspenso;

    public ServicoAbrangencia(String servico, int cep_inicial, int cep_final, int faixa_suspensa, int servico_suspenso) {
        this.servico = servico;
        this.cep_inicial = cep_inicial;
        this.cep_final = cep_final;
        this.faixa_suspensa = faixa_suspensa;
        this.servico_suspenso = servico_suspenso;
    }

    public int getFaixa_suspensa() {
        return faixa_suspensa;
    }

    public void setFaixa_suspensa(int faixa_suspensa) {
        this.faixa_suspensa = faixa_suspensa;
    }

    public int getServico_suspenso() {
        return servico_suspenso;
    }

    public void setServico_suspenso(int servico_suspenso) {
        this.servico_suspenso = servico_suspenso;
    }

    public int getCep_final() {
        return cep_final;
    }

    public void setCep_final(int cep_final) {
        this.cep_final = cep_final;
    }

    public int getCep_inicial() {
        return cep_inicial;
    }

    public void setCep_inicial(int cep_inicial) {
        this.cep_inicial = cep_inicial;
    }

    public String getServico() {
        return servico;
    }

    public void setServico(String servico) {
        this.servico = servico;
    }
    
}
