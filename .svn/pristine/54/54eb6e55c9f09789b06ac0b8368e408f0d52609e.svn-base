package com.portal.componentes.nfe;

import java.util.Map;

public class ParametrosFormularioNFE {

    private final String URL = "http://www.nfe.fazenda.gov.br/portal/consulta.aspx?tipoConsulta=completa&tipoConteudo=XbSeqxE8pl8=";
    private final String AGENT = "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/53.0.2785.143 Safari/537.36";
    private Map<String, String> cookies;
    private String viewStateGenerate;
    private String eventValidation;
    private String viewState;
    private String token;
    private String captchaSom;

    public Map<String, String> getCookies() {
        return cookies;
    }

    public void setCookies(Map<String, String> cookies) {
        this.cookies = cookies;
    }

    public String getViewStateGenerate() {
        return viewStateGenerate;
    }

    public void setViewStateGenerate(String viewStateGenerate) {
        this.viewStateGenerate = viewStateGenerate;
    }

    public String getEventValidation() {
        return eventValidation;
    }

    public void setEventValidation(String eventValidation) {
        this.eventValidation = eventValidation;
    }

    public String getViewState() {
        return viewState;
    }

    public void setViewState(String viewState) {
        this.viewState = viewState;
    }

    public String getToken() {
        return token;
    }

    public void setToken(String token) {
        this.token = token;
    }

    public String getCaptchaSom() {
        return captchaSom;
    }

    public void setCaptchaSom(String captchaSom) {
        this.captchaSom = captchaSom;
    }

    public String getURL() {
        return URL;
    }

    public String getAGENT() {
        return AGENT;
    }

}
