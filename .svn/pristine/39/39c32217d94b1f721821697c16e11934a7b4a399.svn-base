package Entidade;

import java.sql.Timestamp;

/**
 *
 * @author RICADINHO
 */
public class LogColeta {

    private int id;
    private int idColeta;
    private int idUsuario;
    private String nomeUsuario;
    private Timestamp dataHora;
    private String acao;

    public LogColeta(int id, int idColeta, int idUsuario, String nomeUsuario, Timestamp dataHora, String acao) {
        this.id = id;
        this.idColeta = idColeta;
        this.idUsuario = idUsuario;
        this.nomeUsuario = nomeUsuario;
        this.dataHora = dataHora;
        this.acao = acao;
    }

    public String getAcao() {
        return acao;
    }

    public void setAcao(String acao) {
        this.acao = acao;
    }

    public Timestamp getDataHora() {
        return dataHora;
    }

    public void setDataHora(Timestamp dataHora) {
        this.dataHora = dataHora;
    }

    public int getId() {
        return id;
    }

    public void setId(int id) {
        this.id = id;
    }

    public int getIdColeta() {
        return idColeta;
    }

    public void setIdColeta(int idColeta) {
        this.idColeta = idColeta;
    }

    public int getIdUsuario() {
        return idUsuario;
    }

    public void setIdUsuario(int idUsuario) {
        this.idUsuario = idUsuario;
    }

    public String getNomeUsuario() {
        return nomeUsuario;
    }

    public void setNomeUsuario(String nomeUsuario) {
        this.nomeUsuario = nomeUsuario;
    }

}
